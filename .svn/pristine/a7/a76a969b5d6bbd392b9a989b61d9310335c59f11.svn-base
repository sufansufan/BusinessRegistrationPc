import request from '@/utils/request'

// 获取教室列表
export const getClassroomList = data =>
  request({
    url: '/a/backgroundSystem/classroom/findClassroomList',
    data
  })

// 新增编辑保存教室
export const editClassroom = data =>
  request({
    url: '/a/backgroundSystem/classroom/editClassroom',
    data
  })

// 删除教室
export const deleteClassroom = data =>
  request({
    url: '/a/backgroundSystem/classroom/deleteClassroom',
    data
  })

// 教室状态修改
export const editClassroomStatus = data =>
  request({
    url: '/a/backgroundSystem/classroom/editClassroomStatus',
    data
  })
