import request from '@/utils/request'

// 按市级查询
export const getIncomeListByCity = data =>
  request({
    url: '/a/financeDateStatistics/findIncomeListByCity',
    data
  })

// 按校区查询
export const getIncomeListByCampus = data =>
  request({
    url: '/a/financeDateStatistics/findIncomeListByCampus',
    data
  })
